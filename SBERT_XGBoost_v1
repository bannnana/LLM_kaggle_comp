   "source": [
    "def embed_text(df_col, model):\n",
    "    sentences = df_col.tolist()\n",
    "    embeddings = model.encode(sentences, convert_to_numpy=True, show_progress_bar=True)\n",
    "    return list(embeddings)"
   ]
