    "    test_df[col + '_embedded'] = embed_text(test_df[col], sbert)\n",
    "\n",
    "# Perform joint embeddings\n",
    "train_df['prompt_resp_a_embeded'] = joint_embed_text(train_df['prompt'], train_df['response_a'], sbert)\n",
