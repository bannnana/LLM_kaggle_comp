    "def new_col_name(col1, col2):\n",
    "    new_col1 = col1.replace('_embedded','')\n",
    "    new_col2 = col2.replace('_embedded','')\n",
    "    col_name = f'_{new_col1}_{new_col2}'\n",
    "\n",
    "    return col_name"
   ]
