    "# Add columns with cosine similarity and abs difference\n",
    "col_pairs = [['prompt_embedded', 'response_a_embedded'],\n",
    "             ['prompt_embedded', 'response_b_embedded'],\n",
    "             ['response_a_embedded', 'response_b_embedded']\n",
