   "source": [
    "def calculate_similarity(col1, col2):\n",
    "    a = np.vstack(col1.to_numpy())\n",
    "    b = np.vstack(col2.to_numpy())\n",
    "    sim = cosine_similarity(a, b)\n",
    "    sim_final = np.diag(sim)\n",
    "\n",
    "    abs_diff = np.abs(a - b).mean(axis=1)\n",
    "    \n",
    "    return sim_final, abs_diff"
   ]
  },
  {
